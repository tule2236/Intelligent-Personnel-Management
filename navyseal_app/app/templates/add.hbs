<div class="container-fluid">
    <div class="row">
      <div class="col-sm-2">
          {{personnel-nav}}
      </div>
      <div class="col-sm-10">

        <form class="form-horizontal">
        <fieldset>

        <!-- Form Name -->
        <legend>Add New Personnel</legend>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="id">ID</label>
          <div class="col-md-4">
          <input id="id" name="id" type="text" placeholder="" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="firstName">First Name</label>
          <div class="col-md-4">
          <input id="firstName" name="firstName" type="text" placeholder="" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="lastName">Last Name</label>
          <div class="col-md-4">
          <input id="lastName" name="lastName" type="text" placeholder="" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="dob">Date of Birth</label>
          <div class="col-md-4">
          <input id="dob" name="dob" type="text" placeholder="MM/DD/YYYY" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="years">Enlistment Year</label>
          <div class="col-md-4">
          <input id="years" name="years" type="text" placeholder="YYYY" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-md-4 control-label" for="navsogYear">NavSOG Class</label>
          <div class="col-md-4">
          <input id="navsogYear" name="navsogYear" type="text" placeholder="YYYY" class="form-control input-md" required="">

          </div>
        </div>

        <!-- Select Basic -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="civEd">Civilian Education</label>
          <div class="col-md-4">
            <select id="civEd" name="civEd" class="form-control">
              <option value="1">High School</option>
              <option value="2">Undergraduate</option>
              <option value="3">Graduate</option>
            </select>
          </div>
        </div>

        <!-- Select Basic -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="rank">Rank</label>
          <div class="col-md-4">
            <select id="rank" name="rank" class="form-control">
              <option value="1">Officer</option>
              <option value="2">Enlisted</option>
            </select>
          </div>
        </div>

        <!-- Select Basic -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="typeUnit">Type Unit</label>
          <div class="col-md-4">
            <select id="typeUnit" name="typeUnit" class="form-control">
              <option value="1">Seal</option>
              <option value="2">Diving</option>
              <option value="3">EOD</option>
              <option value="4">Special Boat</option>
            </select>
          </div>
        </div>

        <!-- Multiple Checkboxes (inline) -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="milEd">Military Education</label>
          <div class="col-md-4">
            <label class="checkbox-inline" for="milEd-0">
              <input type="checkbox" name="milEd" id="milEd-0" value="1">
              CC
            </label>
            <label class="checkbox-inline" for="milEd-1">
              <input type="checkbox" name="milEd" id="milEd-1" value="2">
              Basic
            </label>
            <label class="checkbox-inline" for="milEd-2">
              <input type="checkbox" name="milEd" id="milEd-2" value="3">
              GSC
            </label>
            <label class="checkbox-inline" for="milEd-3">
              <input type="checkbox" name="milEd" id="milEd-3" value="4">
              MNSA
            </label>
          </div>
        </div>

        <!-- Multiple Checkboxes (inline) -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="positions">Positions Held</label>
          <div class="col-md-4">
            <label class="checkbox-inline" for="positions-0">
              <input type="checkbox" name="positions" id="positions-0" value="1">
              Team Leader
            </label>
            <label class="checkbox-inline" for="positions-1">
              <input type="checkbox" name="positions" id="positions-1" value="2">
              Commanding Officer
            </label>
            <label class="checkbox-inline" for="positions-2">
              <input type="checkbox" name="positions" id="positions-2" value="3">
              Ex-officer
            </label>
            <label class="checkbox-inline" for="positions-3">
              <input type="checkbox" name="positions" id="positions-3" value="4">
              Team Member
            </label>
          </div>
        </div>

        <!-- Button (Double) -->
        <div class="form-group">
          <label class="col-md-4 control-label" for="button1"></label>
          <div class="col-md-8">
            <button id="button1" name="button1" class="btn btn-primary">Submit</button>
            <button id="button2id" name="button2id" class="btn btn-default">Cancel</button>
          </div>
        </div>

        </fieldset>
        </form>




      </div>
    </div>
</div>
